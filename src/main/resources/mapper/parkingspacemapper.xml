<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.smartairportsystem.mapper.parkingspacemapper">
    <resultMap id="BaseResultMap" type="com.example.smartairportsystem.entity.parkingspace">
    </resultMap>
    <select id="listEmptyParkingspace" resultType="com.example.smartairportsystem.entity.parkingspace">
        select * from parkingspace ps where parkingspaceid not in (select po.parkingspaceid from parkingorder po)
    </select>
</mapper>