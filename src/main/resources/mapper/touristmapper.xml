<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.smartairportsystem.mapper.touristmapper">
    <resultMap id="BaseResultMap" type="com.example.smartairportsystem.entity.tourist">
    </resultMap>
    <insert id="logupNewTourist" parameterType="com.example.smartairportsystem.entity.tourist">
        insert into tourist(touristid,nickname,vip,realname,passwords) values(#{touristid},#{nickname},#{vip},#{realname},sha1(#{passwords}))
    </insert>
</mapper>